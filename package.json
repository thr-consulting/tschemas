{
  "name": "tschemas",
  "version": "0.1.7",
  "description": "Useful yup schema shortcuts",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prepublish": "npm run build",
    "test": "npm run build && mocha --compilers js:babel-core/register",
    "gendocs": "jsdoc -c .jsdoc && jsdoc2md -c .jsdoc --files lib/* >> docs/INDEX.md",
    "gendocs:explain": "jsdoc -X -c .jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/tschemas.git"
  },
  "keywords": [
    "yup",
    "schema"
  ],
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.0.2",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.4.2",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "moment": "^2.18.1",
    "social-insurance-number": "^0.0.2",
    "yup": "^0.21.3"
  }
}
