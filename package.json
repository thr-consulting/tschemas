{
	"name": "tschemas",
	"version": "0.1.5",
	"description": "Useful yup schema shortcuts",
	"main": "lib/index.js",
	"scripts": {
		"build": "webpack --config webpack.config.js",
		"prepublish": "npm run build",
		"test": "npm run build && mocha --compilers js:babel-core/register",
		"gendocs": "jsdoc -c .jsdoc && jsdoc2md -c .jsdoc --files src/* >> docs/INDEX.md",
		"gendocs:explain": "jsdoc -X -c .jsdoc"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thr-consulting/tschemas.git"
	},
	"keywords": [
		"yup",
		"schema"
	],
	"author": "Mike Kornelson <mike@durbn.net>",
	"license": "MIT",
	"devDependencies": {
		"babel-core": "^6.24.1",
		"babel-loader": "^6.4.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-1": "^6.24.1",
		"chai": "^3.5.0",
		"ink-docstrap": "^1.3.0",
		"jsdoc": "^3.4.3",
		"jsdoc-babel": "^0.3.0",
		"jsdoc-to-markdown": "^3.0.0",
		"mocha": "^3.2.0",
		"webpack": "^2.3.3",
		"webpack-node-externals": "^1.5.4"
	},
	"dependencies": {
		"escape-string-regexp": "^1.0.5",
		"moment": "^2.18.1",
		"social-insurance-number": "^0.0.2",
		"yup": "^0.21.3"
	}
}
